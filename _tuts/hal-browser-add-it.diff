+HAL.Views.Resource = Backbone.View.extend({
+  initialize: function(opts) {
+    var self = this;
+
+    this.vent = opts.vent;
+
+    this.vent.bind('response', function(e) {
+      self.render(new HAL.Models.Resource(e.resource));
+    });
+
+    this.vent.bind('fail-response', function(e) {
+        try {
